{
  "name": "ai-google-calendar-assistant",
  "displayName": "Google AI Personal Assistant",
  "version": "1.0.139",
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "check": "npx ultracite check",
    "dev": "bun --watch app.ts",
    "fix": "npx ultracite fix --unsafe",
    "format": "npx biome check --write . && npx biome fix --write .",
    "sort": "npx sort-package-json",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start:node": "node dist/app.js",
    "test": "bun run jest",
    "test:ci": "bun run jest --ci --coverage --maxWorkers=2",
    "test:coverage": "bun run jest --coverage",
    "test:watch": "bun run jest --watch",
    "update:db:types": "npx supabase gen types typescript --project-id vdwjfekcsnurtjsieojv --schema public > database.types.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/generative-ai": "^0.24.1",
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/commands": "^1.2.0",
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/menu": "^1.3.1",
    "@grammyjs/runner": "^2.0.3",
    "@jest/globals": "^30.2.0",
    "@lemonsqueezy/lemonsqueezy.js": "^4.0.0",
    "@openai/agents": "^0.3.7",
    "@openai/agents-core": "^0.3.7",
    "@react-email/components": "^1.0.4",
    "@slack/bolt": "^4.6.0",
    "@supabase/supabase-js": "^2.80.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/lodash-es": "^4.17.12",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.8",
    "@types/validator": "^13.15.4",
    "bullmq": "^5.66.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "googleapis": "^105.0.0",
    "grammy": "^1.38.3",
    "helmet": "^8.0.0",
    "i18next": "^25.7.3",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.22",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "openai": "^4.77.0",
    "pm2": "^5.4.3",
    "posthog-node": "^5.21.0",
    "react": "^19.2.3",
    "resend": "^6.6.0",
    "sharp": "^0.34.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "validator": "^13.15.22",
    "winston": "^3.19.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.2.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "nodemon": "^3.1.10",
    "supabase": "^2.58.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript-eslint": "^8.46.3",
    "ultracite": "^5.6.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "supabase",
      "unrs-resolver"
    ]
  }
}
