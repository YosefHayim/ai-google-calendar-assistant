{
  "name": "ai-google-calendar-assistant",
  "displayName": "Google AI Personal Assistant",
  "version": "5.27.0",
  "type": "commonjs",
  "main": "app.js",
  "scripts": {
    "check": "npx ultracite check",
    "dev": "bun --watch app.ts",
    "fix": "npx ultracite fix --unsafe",
    "format": "npx biome check --write . && npx biome fix --write .",
    "sort": "npx sort-package-json",
    "start": "bun app.ts",
    "test": "bun run jest",
    "test:ci": "bun run jest --ci --coverage --maxWorkers=2",
    "test:coverage": "bun run jest --coverage",
    "test:watch": "bun run jest --watch",
    "update:db:types": "npx supabase gen types typescript --project-id vdwjfekcsnurtjsieojv --schema public > database.types.ts"
  },
  "dependencies": {
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/commands": "^1.2.0",
    "@grammyjs/conversations": "^2.1.0",
    "@grammyjs/menu": "^1.3.1",
    "@grammyjs/runner": "^2.0.3",
    "@jest/globals": "^30.2.0",
    "@openai/agents": "^0.3.7",
    "@openai/agents-core": "^0.3.7",
    "@openai/agents-realtime": "^0.3.7",
    "@supabase/supabase-js": "^2.80.0",
    "@types/lodash-es": "^4.17.12",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "googleapis": "^105.0.0",
    "grammy": "^1.38.3",
    "i18next": "^25.7.3",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lodash-es": "^4.17.22",
    "stripe": "^18.5.0",
    "supabase": "^2.58.3",
    "validator": "^13.15.22",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@eslint/js": "^9.39.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.4",
    "babel-jest": "^30.2.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.6",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "task-master-ai": "^0.40.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "ultracite": "5.6.4",
    "winston": "^3.19.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "supabase",
      "unrs-resolver"
    ]
  }
}
